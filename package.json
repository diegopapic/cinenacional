{
  "name": "cinenacional",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "node scripts/compile-project-code.js",
    "structure": "node scripts/generate-structure.js",
    "update-docs": "npm run structure && npm run compile",
    "precommit": "npm run update-docs",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "db:export": "node scripts/export-db-structure.js",
    "docs:build": "documentation build \"src/**/*.{ts,tsx,js,jsx}\" --shallow -f md -o docs/api.md",
    "docs:html": "documentation build \"src/**/*.{ts,tsx,js,jsx}\" --shallow -f html -o docs/html",
    "docs:jsdoc": "jsdoc src -r -d docs/jsdoc",
    "docs:types": "typedoc --out docs/typedoc src"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.80.6",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.513.0",
    "mysql2": "^3.14.3",
    "next": "^14.2.13",
    "next-cloudinary": "^6.16.0",
    "node-fetch": "^3.3.2",
    "php-unserialize": "^0.0.1",
    "prisma": "^6.9.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.57"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "10.4.20",
    "documentation": "^14.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "husky": "^9.1.7",
    "jsdoc": "^4.0.4",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.13",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.10",
    "typescript": "^5.3.0"
  }
}
